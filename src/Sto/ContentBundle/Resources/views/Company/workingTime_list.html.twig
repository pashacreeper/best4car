{% if workingTime %}
    {% for item in workingTime %}
        <span>{{ item.fromTime|date('H:i') }}-{{ item.tillTime|date('H:i') }} ({{ item.days | working_time_days_number_to_array | working_time_days_array_to_string }})</span>
        {% if not loop.last %}<br>{% endif %}
    {% endfor %}
{% endif %}
