{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*@dev",
        "doctrine/orm": "2.3.*@dev",
        "doctrine/doctrine-bundle": "1.1.*@dev",
        "doctrine/data-fixtures": "1.0.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "twig/extensions": "1.0.*@dev",
        "symfony/assetic-bundle": "2.1.*@dev",
        "symfony/swiftmailer-bundle": "2.1.*@dev",
        "symfony/monolog-bundle": "2.1.*@dev",
        "sensio/distribution-bundle": "2.1.*@dev",
        "sensio/framework-extra-bundle": "2.1.*@dev",
        "sensio/generator-bundle": "2.1.*@dev",
        "jms/security-extra-bundle": "1.4.*@dev",
        "jms/di-extra-bundle": "1.3.*@dev",
        "kriswallsmith/assetic": "1.1.*@dev",

        "jms/serializer-bundle": "0.12.*@dev",

        "friendsofsymfony/rest-bundle": "0.11.*@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",

        "nelmio/api-doc-bundle": "2.1.*@dev",

        "knplabs/knp-menu": "2.0.*@dev",
        "knplabs/knp-menu-bundle": "2.0.*@dev",
        "knplabs/knp-paginator-bundle": "2.3.*@dev",
        "knplabs/gaufrette": "0.2.*@dev",
        "knplabs/knp-gaufrette-bundle": "0.2.*@dev",

        "leafo/lessphp": "0.3.*@dev",
        "twitter/bootstrap": "v2.3.1",
        "mopa/bootstrap-bundle": "2.1.*@dev",

        "vich/uploader-bundle": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "leafo/lessphp",
                "source": {
                    "url": "git://github.com/leafo/lessphp.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/leafo/lessphp/zipball/master",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package":
            {
                "name": "twitter/bootstrap",
                "version": "v2.3.1",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "v2.3.1"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/v2.3.1",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "config": {
        "process-timeout": 900
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink"
    }
}
